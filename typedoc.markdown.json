{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/sdk.ts",
    "src/sdk/index.ts"
  ],
  "entryPointStrategy": "expand",
  "out": "docs-markdown",
  "tsconfig": "tsconfig.typedoc.json",
  "includeVersion": true,
  "readme": "README-SDK.md",
  "name": "Runloop SDK API Reference",
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": false,
  "excludeExternals": false,
  "plugin": ["typedoc-plugin-markdown"],
  "flattenOutputFiles": true,
  "entryFileName": "README.md",
  "categorizeByGroup": true,
  "searchInComments": true,
  "sort": [
    "source-order"
  ],
  "kindSortOrder": [
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Method",
    "Accessor",
    "Function",
    "Variable"
  ],
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": true
  },
  "validation": {
    "invalidLink": true,
    "notDocumented": false
  }
}

